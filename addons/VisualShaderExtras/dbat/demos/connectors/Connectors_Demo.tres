code = "shader_type canvas_item;


// Circle

		float sdCircle(vec2 pos, float r) {
			return length(pos) - r;
		}
	

void fragment() {
// Input:18
	vec2 n_out18p0 = UV;


	vec4 n_out9p0;
// ConnectOne:9
	{
		n_out9p0 = vec4(n_out18p0.xy,0.,0.);
	}


	float n_out2p0;
// Circle:2
	vec2 n_in2p1 = vec2(0.50000, 0.50000);
	float n_in2p2 = -0.00500;
	{
		n_out2p0 = sdCircle(UV.xy - n_in2p1.xy, n_in2p2);
	}


// ColorConstant:3
	vec4 n_out3p0 = vec4(1.000000, 0.386878, 0.342078, 1.000000);


// ColorOp:5
	vec3 n_out5p0 = abs(vec3(n_out3p0.xyz) - vec3(n_out2p0));


	bool n_out6p0;
	float n_out6p1;
	int n_out6p2;
	vec2 n_out6p3;
	vec3 n_out6p4;
	vec4 n_out6p5;
	mat4 n_out6p6;
// ConnectMany:6
	{
		n_out6p1 = n_out2p0;
		n_out6p4 = n_out5p0;
	}


// Mix:10
	float n_in10p2 = 0.50000;
	vec3 n_out10p0 = mix(vec3(n_out9p0.xyz), n_out6p4, n_in10p2);


// FloatOp:7
	float n_in7p0 = 0.75700;
	float n_out7p0 = n_in7p0 - n_out6p1;


// Output:0
	COLOR.rgb = n_out10p0;
	COLOR.a = n_out7p0;
	NORMAL_MAP_DEPTH = n_out9p0.x;


}
