code = "shader_type spatial;
render_mode blend_mix, depth_draw_opaque, cull_back, diffuse_lambert, specular_schlick_ggx;

uniform vec4 water_color : source_color = vec4(0.181027, 0.923298, 0.883855, 1.000000);
uniform sampler2D screen_tex_frg_2 : hint_screen_texture;
uniform vec4 absorption_color : source_color = vec4(0.700000, 0.300000, 0.400000, 1.000000);
uniform float absorption_coeff : hint_range(0, 5) = 0.5;
uniform sampler2D depth_tex_frg_12 : hint_depth_texture;


// VolumeColorAbsorption

		vec4 volume_absorption(vec4 input_col, vec4 abs_col, float abs_coeff, float abs_depth) {
			return input_col - (1.0 - exp(-abs_coeff * abs_depth)) * abs_col;
		}
	

void fragment() {
// ColorConstant:10
	vec4 n_out10p0 = vec4(1.000000, 1.000000, 1.000000, 1.000000);


// ColorParameter:9
	vec4 n_out9p0 = water_color;


// VectorOp:11
	vec4 n_out11p0 = n_out10p0 - n_out9p0;


	vec4 n_out2p0;
// Texture2D:2
	n_out2p0 = texture(screen_tex_frg_2, SCREEN_UV);


// ColorParameter:3
	vec4 n_out3p0 = absorption_color;


// FloatParameter:6
	float n_out6p0 = absorption_coeff;


	float n_out12p0;
// LinearSceneDepth:12
	{
		float __log_depth = textureLod(depth_tex_frg_12, SCREEN_UV, 0.0).x;
	vec4 __depth_view = INV_PROJECTION_MATRIX * vec4(SCREEN_UV * 2.0 - 1.0, __log_depth, 1.0);
		__depth_view.xyz /= __depth_view.w;
		n_out12p0 = -__depth_view.z;
	}


	vec4 n_out5p0;
// VolumeColorAbsorption:5
	{
		n_out5p0 = volume_absorption(n_out2p0, n_out3p0, n_out6p0, n_out12p0);
	}


// VectorOp:8
	vec4 n_out8p0 = n_out11p0 * n_out5p0;


// VectorDecompose:13
	float n_out13p0 = n_out8p0.x;
	float n_out13p1 = n_out8p0.y;
	float n_out13p2 = n_out8p0.z;
	float n_out13p3 = n_out8p0.w;


// Output:0
	ALBEDO = vec3(n_out8p0.xyz);
	ALPHA = n_out13p3;


}
