	vec2 n_out2p0;
// UVtoSphereCoord:2
	{
		n_out2p0.xy = uv_to_sphere_coord(n_out3p0.xyz, n_out4p0.xyz);
	}


// Output:0
	ALBEDO = vec3(n_out2p0, 0.0);


}
